<p id="notice"><%= notice %></p>

<h1>Transactionbills</h1>

<table>
  <thead>
    <tr>
      <th>Bill</th>
      <th>Vendortransaction</th>
      <th>Amount</th>
      <th>Balance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactionbills.each do |transactionbill| %>
      <tr>
        <td><%= transactionbill.bill %></td>
        <td><%= transactionbill.vendortransaction %></td>
        <td><%= transactionbill.amount %></td>
        <td><%= transactionbill.balance %></td>
        <td><%= link_to 'Show', transactionbill %></td>
        <td><%= link_to 'Edit', edit_transactionbill_path(transactionbill) %></td>
        <td><%= link_to 'Destroy', transactionbill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transactionbill', new_transactionbill_path %>
